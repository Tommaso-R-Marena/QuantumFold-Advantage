   "source": [
    "## 1. Generate Sample Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Set random seed for reproducibility\n",
    "np.random.seed(42)\n",
    "\n",
    "# Create synthetic protein structure (alpha helix)\n",
    "n_residues = 50\n",
    "t = np.linspace(0, 4*np.pi, n_residues)\n",
    "\n",
    "# Alpha helix geometry (realistic parameters)\n",
    "radius = 2.3  # Angstroms\n",
    "pitch = 1.5   # Angstroms\n",
    "\n",
    "coords = np.zeros((n_residues, 3))\n",
    "coords[:, 0] = radius * np.cos(t)\n",
    "coords[:, 1] = radius * np.sin(t)\n",
    "coords[:, 2] = pitch * t\n",
    "\n",
    "# Add realistic noise\n",
    "coords += np.random.randn(n_residues, 3) * 0.2\n",
    "\n",
    "print(f'üìä Generated structure with {n_residues} residues')\n",
    "print(f'üìè Dimensions: {coords.min(axis=0)} to {coords.max(axis=0)}')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 2. 3D Structure Visualization"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "fig = plt.figure(figsize=(15, 5))\n",
    "\n",
    "# View 1: Backbone trace\n",
    "ax1 = fig.add_subplot(131, projection='3d')\n",
    "ax1.plot(coords[:, 0], coords[:, 1], coords[:, 2], 'b-', linewidth=2, alpha=0.7)\n",
    "ax1.scatter(coords[:, 0], coords[:, 1], coords[:, 2], c=range(n_residues), \n",
    "           cmap='viridis', s=50, alpha=0.8)\n",
    "ax1.set_xlabel('X (√Ö)')\n",
    "ax1.set_ylabel('Y (√Ö)')\n",
    "ax1.set_zlabel('Z (√Ö)')\n",
    "ax1.set_title('Backbone Trace', fontweight='bold')\n",
    "\n",
    "# View 2: C-alpha spheres\n",
    "ax2 = fig.add_subplot(132, projection='3d')\n",
    "scatter = ax2.scatter(coords[:, 0], coords[:, 1], coords[:, 2], \n",
    "                     c=range(n_residues), cmap='plasma', s=200, alpha=0.6)\n",
    "ax2.set_xlabel('X (√Ö)')\n",
    "ax2.set_ylabel('Y (√Ö)')\n",
    "ax2.set_zlabel('Z (√Ö)')\n",
    "ax2.set_title('C-alpha Representation', fontweight='bold')\n",
    "plt.colorbar(scatter, ax=ax2, label='Residue Index', shrink=0.8)\n",
    "\n",
    "# View 3: Tube representation\n",
    "ax3 = fig.add_subplot(133, projection='3d')\n",
    "for i in range(len(coords)-1):\n",
    "    ax3.plot(coords[i:i+2, 0], coords[i:i+2, 1], coords[i:i+2, 2],\n",
    "            color=plt.cm.coolwarm(i/n_residues), linewidth=4, alpha=0.8)\n",
    "ax3.set_xlabel('X (√Ö)')\n",
    "ax3.set_ylabel('Y (√Ö)')\n",
    "ax3.set_zlabel('Z (√Ö)')\n",
    "ax3.set_title('Tube Representation', fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('3d_structures.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()\n",
    "\n",
    "print('‚úÖ 3D visualizations created')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 3. Distance and Contact Maps"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate distance matrix\n",
    "distances = np.sqrt(np.sum((coords[:, None, :] - coords[None, :, :]) ** 2, axis=2))\n",
    "\n",
    "# Create contact map (threshold at 8 √Ö)\n",
    "contact_threshold = 8.0\n",
    "contacts = (distances < contact_threshold).astype(float)\n",
    "np.fill_diagonal(contacts, 0)  # Remove self-contacts\n",
    "\n",
    "fig, axes = plt.subplots(1, 3, figsize=(18, 5))\n",
    "\n",
    "# Distance map\n",
    "im1 = axes[0].imshow(distances, cmap='viridis', interpolation='nearest')\n",
    "axes[0].set_title('Distance Map', fontsize=14, fontweight='bold')\n",
    "axes[0].set_xlabel('Residue Index')\n",
    "axes[0].set_ylabel('Residue Index')\n",
    "plt.colorbar(im1, ax=axes[0], label='Distance (√Ö)', shrink=0.8)\n",
    "\n",
    "# Contact map\n",
    "im2 = axes[1].imshow(contacts, cmap='RdYlBu_r', interpolation='nearest', vmin=0, vmax=1)\n",
    "axes[1].set_title(f'Contact Map (<{contact_threshold}√Ö)', fontsize=14, fontweight='bold')\n",
    "axes[1].set_xlabel('Residue Index')\n",
    "axes[1].set_ylabel('Residue Index')\n",
    "plt.colorbar(im2, ax=axes[1], label='Contact', shrink=0.8)\n",
    "\n",
    "# Distance difference (comparing with slightly perturbed structure)\n",
    "coords_alt = coords + np.random.randn(*coords.shape) * 1.0\n",
    "distances_alt = np.sqrt(np.sum((coords_alt[:, None, :] - coords_alt[None, :, :]) ** 2, axis=2))\n",
    "dist_diff = np.abs(distances - distances_alt)\n",
    "\n",
    "im3 = axes[2].imshow(dist_diff, cmap='hot', interpolation='nearest')\n",
    "axes[2].set_title('Distance Difference', fontsize=14, fontweight='bold')\n",
    "axes[2].set_xlabel('Residue Index')\n",
    "axes[2].set_ylabel('Residue Index')\n",
    "plt.colorbar(im3, ax=axes[2], label='|Œî Distance| (√Ö)', shrink=0.8)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('distance_maps.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()\n",
    "\n",
    "# Statistics\n",
    "n_contacts = np.sum(contacts) / 2  # Divide by 2 (symmetric matrix)\n",
    "print(f'\\nüìä Contact Statistics:')\n",
    "print(f'   Total contacts (<{contact_threshold}√Ö): {int(n_contacts)}')\n",
    "print(f'   Contact density: {n_contacts / (n_residues * (n_residues - 1) / 2) * 100:.1f}%')\n",
    "print(f'   Average distance: {distances[np.triu_indices_from(distances, k=1)].mean():.2f} √Ö')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 4. Confidence Heatmap"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate synthetic confidence scores (high confidence beta distribution)\n",
    "np.random.seed(42)\n",
    "confidence_per_residue = np.random.beta(8, 2, n_residues)\n",
    "confidence_pairwise = np.outer(confidence_per_residue, confidence_per_residue)\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(16, 6))\n",
    "\n",
    "# Per-residue confidence\n",
    "colors = plt.cm.RdYlGn(confidence_per_residue)\n",
    "axes[0].bar(range(n_residues), confidence_per_residue, color=colors, \n",
    "           alpha=0.7, edgecolor='black', linewidth=0.5)\n",
    "axes[0].axhline(y=0.7, color='orange', linestyle='--', linewidth=2, \n",
    "               label='High confidence threshold')\n",
    "axes[0].axhline(y=0.5, color='red', linestyle='--', linewidth=2, \n",
    "               label='Low confidence threshold')\n",
    "axes[0].set_xlabel('Residue Index', fontsize=12)\n",
    "axes[0].set_ylabel('Confidence Score', fontsize=12)\n",
    "axes[0].set_title('Per-Residue Confidence', fontsize=14, fontweight='bold')\n",
    "axes[0].set_ylim(0, 1)\n",
    "axes[0].legend()\n",
    "axes[0].grid(alpha=0.3, axis='y')\n",
    "\n",
    "# Pairwise confidence\n",
    "im = axes[1].imshow(confidence_pairwise, cmap='RdYlGn', vmin=0, vmax=1, \n",
    "                   interpolation='nearest')\n",
    "axes[1].set_xlabel('Residue Index', fontsize=12)\n",
    "axes[1].set_ylabel('Residue Index', fontsize=12)\n",
    "axes[1].set_title('Pairwise Distance Confidence', fontsize=14, fontweight='bold')\n",
    "cbar = plt.colorbar(im, ax=axes[1], shrink=0.8)\n",
    "cbar.set_label('Confidence', fontsize=11)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('confidence_maps.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()\n",
    "\n",
    "# Statistics\n",
    "print(f'\\nüìä Confidence Statistics:')\n",
    "print(f'   Mean confidence: {confidence_per_residue.mean():.3f}')\n",
    "print(f'   Min confidence: {confidence_per_residue.min():.3f}')\n",
    "print(f'   Max confidence: {confidence_per_residue.max():.3f}')\n",
    "high_conf_count = (confidence_per_residue > 0.7).sum()\n",
    "print(f'   High confidence residues (>0.7): {high_conf_count}/{n_residues} '
    "        f'({100*high_conf_count/n_residues:.1f}%)')"
   ]
  },
